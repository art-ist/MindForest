	<!--
    For a description of web.config changes for .NET 4.5 see http://go.microsoft.com/fwlink/?LinkId=235367.
  -->
	<system.diagnostics>
		<switches>
			<add name="Data" value="4" />
			<!-- 4=verbose -->
		</switches>
	</system.diagnostics>

	<system.web>
		<customErrors mode="Off">
		</customErrors>
		<authentication mode="None" />
		<!-- handle authentication via custom Auth Attribute -->
		<compilation debug="true" targetFramework="4.5" />
		<httpRuntime targetFramework="4.5" />
	</system.web>

	<system.webServer>
		<modules>
			<remove name="FormsAuthenticationModule" />
		</modules>
		<handlers>
			<!--Debuging-->
			<!--<add name="Browser Link for HTML" path="*.html" verb="*" type="System.Web.StaticFileHandler, System.Web, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" resourceType="File" preCondition="integratedMode" />-->

			<remove name="ExtensionlessUrlHandler-Integrated-4.0" />
			<remove name="OPTIONSVerbHandler" />
			<remove name="TRACEVerbHandler" />
			<add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
		</handlers>
	</system.webServer>

